<grammar xmlns="http://www.w3.org/2001/06/grammar"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.w3.org/2001/06/grammar
                             http://www.w3.org/TR/speech-grammar/grammar.xsd"
         xml:lang="de-DE"
         version="1.0"
         tag-format="semantics/1.0">
  <rule id="MoveIt" scope="public">

    <one-of>
      <item> verschiebe </item>
      <item> bewege </item>      
    </one-of>

    <one-of>
      <item>
        <tag>out.start="1"; </tag>
        1
      </item>
      <item>
        <tag>out.start="2"; </tag>
        2
      </item>
      <item>
        <tag>out.start="3"; </tag>
        3
      </item>
    </one-of>

    <one-of>
      <item> zu </item>
      <item> auf </item>
    </one-of>

    <one-of>
      <item>
        <tag>out.end="1"; </tag>
        1
      </item>
      <item>
        <tag>out.end="2"; </tag>
        2
      </item>
      <item>
        <tag>out.end="3"; </tag>
        3
      </item>
    </one-of>

    <tag>out=out.start + ";" + out.end;</tag>

  </rule>

</grammar>